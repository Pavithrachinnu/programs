import java.util.*;
public class Swap
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a,b;
        a=sc.nextInt();
        b=sc.nextInt();
        a=b-a;
        b=b-a;
        a=a+b;
        System.out.println(a);
        System.out.println(b); 
    }
}
