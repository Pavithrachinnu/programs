import java.util.*;
import java.io.*;
import java.math.*;
class Solution {

    public static void main(String args[]) {
        Scanner in = new Scanner(System.in);
        int N = in.nextInt();
        int M = in.nextInt();
        int count=0;
        for (int i = 0; i < N; i++) {
            String catName = in.next();
            int b = in.nextInt();
            int d = in.nextInt();
            if(M<d&&M>=b)
            count++;
        }
        System.out.println(count);
    }
}
