//Implement a program to find factorial of the given number.

Input Format

an integer value from the user

Constraints

no

Output Format

factorial value

Sample Input 0

5
Sample Output 0

120
Sample Input 1

0
Sample Output 1

1
Sample Input 2

1
Sample Output 2

1//
import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
         int factorial = n;
        if(n==0)
        {
            System.out.print("1");
        }
        else
        {
        for (int i = (n - 1); i >= 1; i--)
      
        {
             factorial = factorial * i;
        }
         System.out.println(factorial);
        }
    }
}


